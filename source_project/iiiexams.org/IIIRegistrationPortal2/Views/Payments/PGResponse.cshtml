<!DOCTYPE HTML>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <title>@ViewBag.Title</title>
    <link href="~/CSS/bootstrap.min.css" rel="stylesheet" />
    <link href="~/CSS/common.css" rel="stylesheet" />
    <link href="~/CSS/jquery-ui.min.css" rel="stylesheet" />
    @*<script src="~/js/RegexDef.js" type="text/javascript"></script>*@
    <script src="~/js/jquery-3.2.1.min.js" type="text/javascript"></script>
    <script src="~/js/bootstrap.min.js" type="text/javascript"></script>
    @*<script src="~/js/jquery-ui.min.js" type="text/javascript"></script>
    <script src="~/js/jquery.validate.min.js" type="text/javascript"></script>
    <script src="~/js/jquery.timepicker.min.js" type="text/javascript"></script>
    <script src="~/js/additional-methods.min.js" type="text/javascript"></script>
    <script src="~/js/app/commonmethods.js" type="text/javascript"></script>
    <script src="~/js/app/examfeespayment.js" type="text/javascript"></script>*@
</head>
<body>
    <div class="wrap">
        <div class="headerdiv">
            <div class="container">
                <div class="row">
                    <div class="col-sm-6 topLeft"><img src="~/images/NSE_III_logo.png" id="homePageBtn" class="cursorPointer" /> </div>
                </div>
            </div>
        </div>
        <div class="navdiv">
            <div class="container">
                <nav class="navbar navbar-default" role="navigation">
                    <div class="container-fluid">
                    </div>
                    <!-- /.container-fluid -->
                </nav>
            </div>
        </div>
        <div class="newsdiv">
            <div class="container">
            </div>
        </div>
        <div class="maindiv">
            <div class="maindivContainer container">
                <div class="row">
                    <div class="col-sm-10 col-sm-offset-1">
                        <div class="row">
                            <div class="col-sm-12">
                                <div class="paymentBox">
                                    <h3>Online Payment Of Examination Fees</h3>
                                    <div class="row">
                                        <div class="col-sm-12" id="PGResponse">
                                            <div class="row mt10">
                                                <div class="col-sm-8">
                                                        <H5>@ViewBag.Status</H5><br />
                                                        @if (ViewBag.ShowTable) {
                                                        <Table class='table table-striped table-bordered table-hover'>
                                                            <thead>
                                                                <tr>
                                                                    <td>Transaction Reference No</td>
                                                                    <td>Payment Gateway Reference No</td>
                                                                    <td>Amount</td>
                                                                    <td>Status Code</td>
                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                <tr>
                                                                    <td>@ViewBag.NSEITTxnsId</td>
                                                                    <td>@ViewBag.PGTxnsId </td>
                                                                    <td>@ViewBag.Amount</td>
                                                                    <td>@ViewBag.StatusCode</td>
                                                                </tr>
                                                            </tbody>
                                                        </Table>
                                                        }
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="footerdiv">
        <div class="container">
            <div class="row">
                <div class="col-sm-6">
                    <p>​All Right Reserved</p>
                </div>
                <div class="col-sm-6 text-right">
                    <p>Portal Developed and Maintained By <img src="~/images/NSEIT_Logo.jpg" width="81" height="47"></p>
                </div>
            </div>
        </div>
    </div>

    <div class="modal" style="display: none">
        <div class="center">
            <img alt="" src="~/Images/Loader.gif" />
        </div>
    </div>
</body>
</html>