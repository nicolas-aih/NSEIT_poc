<!DOCTYPE HTML>
<html>
<head>
    <meta name="viewport" content="width=device-width, initial-scale=4.0">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <title>@ViewBag.Title</title>
    <script language="javascript" type="text/javascript">
        var ServerDate = '@System.DateTime.Now.ToString("dd-MMM-yyyy")';
        var ServerYear = @System.DateTime.Now.Year;
    </script>
    <link href="~/CSS/bootstrap.min.css" rel="stylesheet" />
    <link href="~/CSS/common.css" rel="stylesheet" />
    <link href="~/CSS/jquery-ui.min.css" rel="stylesheet" />
    <script src="~/js/RegexDef.js" type="text/javascript"></script>
    <script src="~/js/jquery-3.2.1.min.js" type="text/javascript"></script>
    <script src="~/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="~/js/jquery-ui.min.js" type="text/javascript"></script>
    <script src="~/js/jquery.validate.min.js" type="text/javascript"></script>
    <script src="~/js/jquery.timepicker.min.js" type="text/javascript"></script>
    <script src="~/js/additional-methods.min.js" type="text/javascript"></script>
    <script src="~/js/app/commonmethods.js" type="text/javascript"></script>
</head>
<body>
    <div class="wrap">
        <div class="headerdiv">
            <div class="container">
                <div class="row">
                    <div class="col-sm-6 topLeft"><img src="~/images/NSE_III_logo.png" id="homePageBtn" class="cursorPointer" /> </div>
                </div>
            </div>
        </div>
        <div class="navdiv"  style="height:10px">
            <div class="container">
                @*<nav class="navbar navbar-default" role="navigation">
                    <div class="container-fluid">
                    </div>
                    <!-- /.container-fluid -->
                </nav>*@
            </div>
        </div>
        <div class="newsdiv" style="height:10px">
            <div class="container">
            </div>
        </div>
        <div class="maindiv">
            <div class="maindivContainer container">
                <div class="row">
                    <div class="col-sm-10 col-sm-offset-1">
                        <div class="row">
                            <div class="col-sm-12">
                                @RenderBody()
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="footerdiv">
        <div class="container">
            <div class="row">
                <div class="col-sm-6">
                    <p>​All Right Reserved</p>
                </div>
                <div class="col-sm-6 text-right">
                    <p>Portal Developed and Maintained By <img src="~/images/NSEIT_Logo.jpg" width="81" height="47"></p>
                </div>
            </div>
        </div>
    </div>

    <div class="modal" style="display: none">
        <div class="center">
            <img alt="" src="~/Images/Loader.gif" />
        </div>
    </div>
</body>
</html>






