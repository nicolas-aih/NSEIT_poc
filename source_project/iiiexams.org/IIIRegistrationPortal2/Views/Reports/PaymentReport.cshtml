
@{
    ViewBag.Title = "PaymentReport";
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
}
<script src="~/js/app/PaymentReport.js" type="text/javascript"></script>
<div id="pnlMain" class="notificationBox">
    <h3>
        Successful Payment Report
    </h3>
    <form id="frmMain">

        <div class="row mt-10">
            <div class="col-sm-12" id="dvApplicantDetails">
                <div class="col-sm-3 form-group">
                    <label for="txtFromDate">From date<span class="mandatory">*</span></label>
                    <input id="txtFromDate" readonly="readonly" style="cursor:pointer;background-color:white;" name="txtFromDate" type="text" class="form-control dtpick" autocomplete="off" />
                </div>
                <div class="col-sm-3 form-group">
                    <label for="txtToDate">Till date<span class="mandatory">*</span></label>
                    <input id="txtToDate" readonly="readonly" style="cursor:pointer;background-color:white;" name="txtToDate" type="text" class="form-control dtpick" autocomplete="off" />
                </div>
                <div class="col-sm-2 form-group">
                    <label>&nbsp;<span class="mandatory">&nbsp;</span></label>
                    <input id="btnexport" type="Submit" class="btn btn-md btn-warning btn-block" value="Export" />
                </div>
                <div class="col-sm-2 form-group">
                    <label>&nbsp;<span class="mandatory">&nbsp;</span></label>
                    <input id="btnClear" type="button" class="btn btn-md btn-warning btn-block" value="Clear" />
                </div>
            </div>
            <div class="col-sm-12" id="ResponseFile">

            </div>
        </div>
    </form>
</div>




