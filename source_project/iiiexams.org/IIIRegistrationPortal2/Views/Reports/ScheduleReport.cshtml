@{
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
}
@*@Scripts.Render("~/js/app/schedulereport")*@
<script src="~/js/app/schedulereport.js" type="text/javascript"></script>

<div id="pnlMain" class="notificationBox">
    <h3>
        Schedule Report
    </h3>
    <form id="frmMain">
        <div class="row mt-10">
            <div class="col-sm-12">
                <div class="col-sm-8 form-group">
                    <label for="cboReportType">Report Type : <span class="mandatory">*</span></label>
                    <Select id="cboReportType" name="cboReportType" class="form-control" required>
                        <option value="">--Select--</option>
                        <option value="1">Schedule - Datewise Count</option>
                        <option value="2">Schedule - Datewise / Centerwise Count</option>
                        <option value="3">Schedule - Datewise / Centerwise / Batchwise Count</option>
                    </Select>
                </div>
            </div>
        </div>
        <div class="row mt-10">
            <div class="col-sm-12" id="dvApplicantDetails">
                <div class="col-sm-4 form-group">
                    <label for="lblFromDate">From date<span class="mandatory">*</span></label>
                    <input id="txtFromDate" readonly="readonly" style="cursor:pointer;background-color:white;" name="txtFromDate" type="text" class="form-control dtpick" autocomplete="off" />
                </div>
                <div class="col-sm-4 form-group">
                    <label for="lblToDate">Till date<span class="mandatory">*</span></label>
                    <input id="txtToDate" readonly="readonly" style="cursor:pointer;background-color:white;" name="txtToDate" type="text" class="form-control dtpick" autocomplete="off" />
                </div>
                <div class="col-sm-4 form-group">
                    <label>&nbsp;<span class="mandatory">&nbsp;</span></label>
                    <input id="btnexport" type="Submit" class="btn btn-md btn-warning btn-block" value="Export" />
                </div>
                <div class="col-sm-9" id="ResponseFile">

                </div>
            </div>
        </div>
    </form>
</div>