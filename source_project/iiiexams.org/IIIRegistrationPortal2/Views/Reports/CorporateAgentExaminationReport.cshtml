@{
    ViewBag.Title = "CorporateAgentExaminationReport";
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
}
@*@Scripts.Render("~/js/app/corporateagentexaminationreport")*@
<script src="~/js/app/corporateagentexaminationreport.js" type="text/javascript"></script>

<div class="notificationBox">
    <h3>Examination Report</h3>
    <form id="frmMain" autocomplete="off">
        <div class="row mt10">
            <div class="col-sm-4">
                <label for="dropdownYear">Select Year</label>
                <select id="dropdownYear" name="dropdownYear" class="form-control"></select>
            </div>
            <div class="col-sm-4">
                <label for="dropdownMonth">Select Month</label>
                <select id="dropdownMonth" name="dropdownMonth" class="form-control"></select>
            </div>
            <div class="col-sm-4">
                <label>&nbsp;</label>
                <input type="submit" value="Download" name="btndownload" id="btndownload" class="btn btn-sm btn-warning btn-block" />
            </div>
        </div>
        <div class="row mt10">
            <div class="col-sm-12" id="ResponseFile">
            </div>
        </div>
    </form>
</div>