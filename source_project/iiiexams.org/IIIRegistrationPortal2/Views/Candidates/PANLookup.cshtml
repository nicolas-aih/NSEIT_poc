
@{
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
}
@*@Scripts.Render("~/js/app/panlookup")*@

@if (PortalSession.RoleCode.In(new string[] { "superadmin", "EB", "TS", "BO" }))
{
    <script src="~/js/app/panlookup_int.js" type="text/javascript"></script>
}
else
{
    <script src="~/js/app/panlookup.js" type="text/javascript"></script>
}
<div id="pnlMain" class="notificationBox">
    <h3>PAN Lookup</h3>
    <div class="row">
        <div class="col-sm-12">
            <div class="row mt10">
                <form id="frmMain" autocomplete="off">
                    <div class="col-sm-3">
                        <div class="form-group">
                            <input id="txtPAN" name="txtPAN" type="text" class="form-control" placeholder="PAN" required maxlength="10" autocomplete="off" />
                        </div>
                    </div>
                    <div class="col-sm-3">
                        <div class="form-group">
                            <input type="submit" class="btn btn-md btn-warning btn-block" value="View">
                        </div>
                    </div>
                </form>
            </div>
            <div class="row mt10">
                <div class="col-sm-12">
                    <div id="data" class="table-responsive" style="display:none">
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

